    <!-- Shared scripts -->
    <script src="../shared/audio-player.js"></script>
    <script src="../shared/navigation.js"></script>
    <script>
        function playTrack(title) {
            if (window.jarchivePlayer) {
                // Update now-playing indicators
                const row = document.querySelector(`.track-entry[data-track="${title}"]`);
                if (row) {
                    row.classList.add('playing');
                    const indicator = row.querySelector('.now-playing-indicator');
                    if (indicator) {
                        indicator.style.display = 'inline-block';
                    }
                }
                
                // Remove playing state from other tracks
                document.querySelectorAll('.track-entry.playing').forEach(entry => {
                    if (entry !== row) {
                        entry.classList.remove('playing');
                        const otherIndicator = entry.querySelector('.now-playing-indicator');
                        if (otherIndicator) {
                            otherIndicator.style.display = 'none';
                        }
                    }
                });
                
                window.jarchivePlayer.playTrackByTitle(title);
            }
        }

        // Update playing indicator when audio player state changes
        document.addEventListener('DOMContentLoaded', () => {
            if (window.jarchivePlayer) {
                const audio = window.jarchivePlayer.audio;
                
                audio.addEventListener('play', () => {
                    const currentTrack = window.jarchivePlayer.playlist[window.jarchivePlayer.currentTrackIndex];
                    if (currentTrack) {
                        const row = document.querySelector(`.track-entry[data-track="${currentTrack.title}"]`);
                        if (row) {
                            row.classList.add('playing');
                            const indicator = row.querySelector('.now-playing-indicator');
                            if (indicator) {
                                indicator.style.display = 'inline-block';
                            }
                        }
                    }
                });
                
                audio.addEventListener('pause', () => {
                    document.querySelectorAll('.track-entry.playing').forEach(entry => {
                        entry.classList.remove('playing');
                        const indicator = entry.querySelector('.now-playing-indicator');
                        if (indicator) {
                            indicator.style.display = 'none';
                        }
                    });
                });
            }
        });
    </script>

