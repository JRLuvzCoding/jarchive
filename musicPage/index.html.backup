    <style>
        /* ========================================
           FORUM-INSPIRED MUSIC ARCHIVE
           Clean, minimal, archival aesthetic
           ======================================== */
        
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        html {
            height: 100%;
        }

        body {
            font-family: 'Courier New', Courier, monospace;
            line-height: 1.6;
            min-height: 100%;
            display: flex;
            flex-direction: column;
            background: #000;
            color: #fff;
        }

        /* SIDEBAR NAVIGATION */
        .jr-side-nav {
            position: fixed;
            left: 0px;
            top: 12px;
            transform: none;
            display: flex !important;
            flex-direction: column;
            gap: 12px;
            z-index: 99999;
            user-select: none;
            pointer-events: auto;
        }

        .jr-side-nav a {
            writing-mode: vertical-rl;
            position: relative;
            transform: rotate(180deg);
            color: rgba(255,255,255,.55);
            text-decoration: none;
            font-size: 14px;
            letter-spacing: .35em;
            text-transform: uppercase;
            padding: 8px 2px;
            border-radius: 999px;
            transition: .2s ease;
        }

        .jr-side-nav a:hover {
            color: rgba(255,255,255,0.82);
            background: transparent;
            box-shadow: none;
        }

        .jr-side-nav a.is-active {
            color:#fff;
            background: transparent;
            outline: none;
            box-shadow: none;
        }

        .jr-side-nav a.is-active::after {
            content:"";
            position:absolute;
            right: -10px;
            top: 50%;
            width: 8px;
            height: 8px;
            border-radius: 999px;
            transform: translateY(-50%);
            background: rgba(255,255,255,.9);
            box-shadow: 0 0 18px rgba(255,255,255,.35);
            animation: navPulse 1.2s ease-in-out infinite;
        }

        @media (max-width: 900px) {
            .jr-side-nav { display:none !important; }
        }

        @keyframes navPulse {
            0%,100% { transform: translateY(-50%) scale(1); opacity: .85; }
            50%     { transform: translateY(-50%) scale(1.35); opacity: 1; }
        }

        /* MAIN CONTAINER */
        .music-container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 2rem 1rem;
            flex: 1;
        }

        /* SECTION HEADER */
        .section-header {
            padding: 0.75rem 1rem;
            margin-bottom: 0;
            background: rgba(255, 255, 255, 0.03);
            border: 1px solid rgba(255, 255, 255, 0.05);
        }

        .section-title {
            font-size: 0.875rem;
            text-transform: uppercase;
            letter-spacing: 0.15em;
            font-weight: bold;
        }

        /* TRACK LIST */
        .track-list {
            display: flex;
            flex-direction: column;
            gap: 0;
        }

        /* TRACK ENTRY (Forum-style row) */
        .track-entry {
            display: grid;
            grid-template-columns: 50px 1fr auto auto;
            gap: 1rem;
            padding: 0.75rem 1rem;
            align-items: center;
            background: rgba(255, 255, 255, 0.02);
            border-top: 1px solid rgba(255, 255, 255, 0.05);
            transition: background 0.2s ease;
        }

        .track-entry:hover {
            background: rgba(255, 255, 255, 0.04);
        }

        .track-entry.playing {
            background: rgba(255, 255, 255, 0.06);
        }

        /* Track Cover (Small) */
        .track-cover {
            width: 50px;
            height: 50px;
            object-fit: cover;
            cursor: pointer;
            opacity: 0.8;
            transition: opacity 0.2s ease;
        }

        .track-cover:hover {
            opacity: 1;
        }

        /* Track Info (Title + Artist) */
        .track-info {
            min-width: 0;
        }

        .track-title {
            font-size: 0.9375rem;
            font-weight: bold;
            text-transform: uppercase;
            letter-spacing: 0.05em;
            margin-bottom: 0.25rem;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        .track-artist {
            font-size: 0.75rem;
            opacity: 0.6;
            text-transform: uppercase;
            letter-spacing: 0.1em;
        }

        .now-playing-indicator {
            display: inline-block;
            width: 6px;
            height: 6px;
            background: #fff;
            border-radius: 50%;
            opacity: 0.8;
        }

        /* Track Metadata (Date + Length) */
        .track-meta {
            display: flex;
            flex-direction: column;
            align-items: flex-end;
            gap: 0.25rem;
            font-size: 0.75rem;
            text-transform: uppercase;
            letter-spacing: 0.1em;
            min-width: 120px;
        }

        .track-date {
            opacity: 0.6;
        }

        .track-length {
            font-weight: bold;
            opacity: 0.8;
        }

        /* Track Actions */
        .track-actions {
            display: flex;
            gap: 0.5rem;
            align-items: center;
        }

        .action-btn {
            padding: 0.375rem 0.75rem;
            font-size: 0.75rem;
            text-transform: uppercase;
            letter-spacing: 0.1em;
            background: rgba(255, 255, 255, 0.05);
            border: 1px solid rgba(255, 255, 255, 0.1);
            color: #fff;
            text-decoration: none;
            cursor: pointer;
            transition: all 0.2s ease;
            font-family: inherit;
            display: inline-flex;
            align-items: center;
            gap: 0.375rem;
        }

        .action-btn:hover {
            background: rgba(255, 255, 255, 0.1);
            border-color: rgba(255, 255, 255, 0.2);
        }

        .action-btn i {
            font-size: 0.875em;
        }

        /* FOOTER */
        .music-footer {
            text-align: center;
            padding: 2rem 1rem;
            padding-bottom: calc(2rem + 120px);
            margin-top: auto;
            font-size: 0.75rem;
            text-transform: uppercase;
            letter-spacing: 0.15em;
        }

        /* RESPONSIVE */
        @media (max-width: 768px) {
            .track-entry {
                grid-template-columns: 40px 1fr;
                gap: 0.75rem;
            }

            .track-cover {
                width: 40px;
                height: 40px;
            }

            .track-meta {
                grid-column: 2;
                align-items: flex-start;
                flex-direction: row;
                gap: 0.75rem;
                margin-top: 0.5rem;
            }

            .track-actions {
                grid-column: 2;
                margin-top: 0.5rem;
            }
        }
    </style>
</head>
<body>

        .middle-header::before,
        .middle-header::after {
            content: attr(data-text);
            position: absolute;
            left: 0;
            width: 100%;
            background: transparent;
            clip-path: polygon(0 0, 100% 10%, 100% 20%, 0 10%);
        }

        .middle-header::before {
            top: -2px;
            text-shadow: 1px 0 rgb(255, 255, 255);
            animation: glitch-top-music 1.5s infinite ease-in-out alternate-reverse;
        }

        .middle-header::after {
            top: 2px;
            text-shadow: -1px 0 rgb(255, 255, 255);
            animation: glitch-bottom-music 1.5s infinite ease-in-out alternate-reverse;
        }

        @keyframes glitch-top-music {
            0% { clip-path: polygon(0 0, 100% 5%, 100% 35%, 0 25%); transform: translate(0); }
            20% { clip-path: polygon(0 10%, 100% 20%, 100% 40%, 0 30%); transform: translate(-1px, -2px); }
            100% { clip-path: polygon(0 40%, 100% 45%, 100% 65%, 0 60%); transform: translate(0); }
        }

        @keyframes glitch-bottom-music {
            0% { clip-path: polygon(0 60%, 100% 65%, 100% 85%, 0 80%); transform: translate(0); }
            20% { clip-path: polygon(0 70%, 100% 75%, 100% 95%, 0 90%); transform: translate(1px, 2px); }
            100% { clip-path: polygon(0 90%, 100% 95%, 100% 100%, 0 100%); transform: translate(0); }
        }

        .content {
            padding: 20px;
            background-color: rgba(0, 0, 0, 0.8);
            min-height: calc(100vh - 160px);
        }

        .music-header {
            text-align: center;
            margin-bottom: 40px;
        }

        .music-header h1 {
            font-size: 3em;
            color: #ffffff;
            text-shadow: 0 0 20px rgba(255, 255, 255, 0.8);
            margin-bottom: 20px;
            text-transform: uppercase;
            letter-spacing: 3px;
        }

        .music-header p {
            font-size: 1.2em;
            color: #cccccc;
            max-width: 600px;
            margin: 0 auto;
        }

        .stats-bar {
            display: flex;
            justify-content: center;
            gap: 30px;
            margin-bottom: 30px;
            flex-wrap: wrap;
        }

        .stat-item {
            background: rgba(255, 255, 255, 0.1);
            padding: 15px 25px;
            border-radius: 10px;
            border: 1px solid rgba(255, 255, 255, 0.2);
            text-align: center;
            backdrop-filter: blur(5px);
        }

        .stat-number {
            font-size: 2em;
            font-weight: bold;
            color: #ff0000;
            text-shadow: 0 0 10px rgba(255, 0, 0, 0.5);
        }

        .stat-label {
            font-size: 0.9em;
            color: #cccccc;
            text-transform: uppercase;
            letter-spacing: 1px;
        }

        .song-list {
            width: 90%;
            margin: 0 auto;
            border-collapse: collapse;
            background: rgba(0, 0, 0, 0.5);
            border-radius: 15px;
            overflow: hidden;
            box-shadow: 0 0 30px rgba(255, 255, 255, 0.1);
        }

        .song-list th {
            background: rgba(255, 255, 255, 0.1);
            padding: 20px 15px;
            text-align: left;
            font-size: 1.1em;
            text-transform: uppercase;
            letter-spacing: 1px;
            color: #ffffff;
            border-bottom: 2px solid rgba(255, 255, 255, 0.2);
        }

        .song-list td {
            padding: 15px;
            text-align: left;
            border-bottom: 1px solid rgba(255, 255, 255, 0.1);
            height: 100px;
            vertical-align: middle;
        }

        .song-list tr {
            transition: all 0.3s ease;
        }

        .song-list tr:hover {
            background: rgba(255, 255, 255, 0.05);
            transform: translateX(5px);
        }

        .song-list tr.playing {
            background: rgba(255, 0, 0, 0.1);
            border-left: 4px solid #ff0000;
        }

        .album-cover {
            width: 80px;
            height: 80px;
            object-fit: cover;
            border-radius: 8px;
            border: 2px solid rgba(255, 255, 255, 0.3);
            transition: all 0.3s ease;
            cursor: pointer;
        }

        .album-cover:hover {
            transform: scale(1.1);
            border-color: #ffffff;
            box-shadow: 0 0 20px rgba(255, 255, 255, 0.5);
        }

        .track-info {
            display: flex;
            flex-direction: column;
            gap: 5px;
        }

        .track-title {
            font-size: 1.2em;
            font-weight: bold;
            color: #ffffff;
            text-shadow: 0 0 5px rgba(255, 255, 255, 0.3);
        }

        .track-artist {
            font-size: 0.9em;
            color: #cccccc;
        }

        .track-actions {
            display: flex;
            gap: 10px;
            align-items: center;
        }

        .action-btn {
            background: transparent;
            border: 1px solid rgba(255, 255, 255, 0.3);
            color: #ffffff;
            padding: 8px 12px;
            border-radius: 4px;
            cursor: pointer;
            transition: all 0.3s ease;
            text-decoration: none;
            display: inline-flex;
            align-items: center;
            gap: 5px;
            font-size: 0.9em;
            position: relative;
            overflow: hidden;
        }

        .action-btn::before {
            content: '';
            position: absolute;
            top: 0;
            left: -100%;
            width: 100%;
            height: 100%;
            background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
            transition: left 0.5s;
        }

        .action-btn:hover::before {
            left: 100%;
        }

        .action-btn:hover {
            background: rgba(255, 255, 255, 0.1);
            border-color: #ffffff;
            transform: translateY(-2px);
            box-shadow: 0 5px 15px rgba(255, 255, 255, 0.2);
        }

        .action-btn:active {
            transform: translateY(0);
        }

        .play-btn {
            background: rgba(255, 255, 255, 0.1);
            border-color: #ffffff;
        }

        .play-btn:hover {
            background: rgba(255, 255, 255, 0.2);
        }

        .download-btn {
            background: rgba(0, 255, 0, 0.1);
            border-color: #00ff00;
            color: #00ff00;
        }

        .download-btn:hover {
            background: rgba(0, 255, 0, 0.2);
            box-shadow: 0 5px 15px rgba(0, 255, 0, 0.3);
        }

        .track-date {
            color: #cccccc;
            font-size: 0.9em;
        }

        .track-length {
            color: #cccccc;
            font-size: 0.9em;
            font-family: 'Courier New', monospace;
        }

        .now-playing-indicator {
            display: inline-block;
            width: 8px;
            height: 8px;
            background: #ff0000;
            border-radius: 50%;
            margin-right: 8px;
            animation: pulse 1.5s infinite;
        }

        @keyframes pulse {
            0%, 100% { opacity: 1; transform: scale(1); }
            50% { opacity: 0.5; transform: scale(1.2); }
        }

        .loading-spinner {
            display: inline-block;
            width: 16px;
            height: 16px;
            border: 2px solid rgba(255, 255, 255, 0.3);
            border-radius: 50%;
            border-top-color: #ffffff;
            animation: spin 1s ease-in-out infinite;
        }

        @keyframes spin {
            to { transform: rotate(360deg); }
        }

        footer {
            text-align: center;
            padding: 20px;
            background-color: rgba(0, 0, 0, 0.9);
            color: white;
            border-top: 2px solid white;
        }

        /* Responsive styles */
        @media (max-width: 768px) {
            .jarchive-dot-net {
                font-size: 1.5em;
                padding: 5px 10px;
            }

            .middle-header {
                font-size: 1.2em;
            }

            .music-header h1 {
                font-size: 2em;
            }

            .stats-bar {
                gap: 15px;
            }

            .stat-item {
                padding: 10px 15px;
            }

            .stat-number {
                font-size: 1.5em;
            }

            .song-list {
                width: 100%;
                font-size: 0.9em;
            }

            .song-list th,
            .song-list td {
                padding: 10px 8px;
            }

            .album-cover {
                width: 60px;
                height: 60px;
            }

            .track-actions {
                flex-direction: column;
                gap: 5px;
            }

            .action-btn {
                padding: 6px 10px;
                font-size: 0.8em;
            }
        }

        @media (max-width: 480px) {
            .song-list {
                font-size: 0.8em;
            }

            .track-title {
                font-size: 1em;
            }

            .album-cover {
                width: 50px;
                height: 50px;
            }

            .stats-bar {
                flex-direction: column;
                align-items: center;
            }
        }

        /* Focus states for accessibility */
        .action-btn:focus,
        .album-cover:focus {
            outline: 2px solid #ff0000;
            outline-offset: 2px;
        }

        /* Loading state */
        .loading {
            opacity: 0.6;
            pointer-events: none;
        }

        .loading .action-btn::after {
            content: '';
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            width: 16px;
            height: 16px;
            border: 2px solid rgba(255, 255, 255, 0.3);
            border-radius: 50%;
            border-top-color: #ffffff;
            animation: spin 1s ease-in-out infinite;
        }
    </style>
</head>

